{
  "name": "@requestnetwork/usage-examples",
  "version": "0.25.0",
  "private": true,
  "description": "Usage examples of Request Network.",
  "keywords": [
    "requestnetwork",
    "usage-examples"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "start": "ts-node src/request-client-js-declarative-request.ts && ts-node src/request-client-js-erc20-request.ts && ts-node src/request-logic-clear-request.ts && ts-node src/request-logic-encrypted-request.ts",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@requestnetwork/data-access": "0.28.0",
    "@requestnetwork/epk-decryption": "0.5.0",
    "@requestnetwork/epk-signature": "0.7.0",
    "@requestnetwork/multi-format": "0.17.0",
    "@requestnetwork/payment-processor": "0.39.0",
    "@requestnetwork/request-client.js": "0.41.0",
    "@requestnetwork/request-logic": "0.28.0",
    "@requestnetwork/transaction-manager": "0.28.0",
    "@requestnetwork/types": "0.37.0",
    "@requestnetwork/utils": "0.37.0",
    "ethers": "5.5.1",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "prettier": "2.2.1",
    "ts-node": "9.0.0",
    "typescript": "4.4.4"
  }
}
