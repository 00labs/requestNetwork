    "@huma-shan/utils": "0.39.0",
    "@openzeppelin/test-helpers": "0.5.6",
    "@truffle/hdwallet-provider": "1.2.3",
    "@types/jest": "26.0.13",
    "@types/node": "14.14.16",
    "ethers": "5.5.1",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "web3-eth": "1.3.6"
  }
