{
  "name": "@requestnetwork/request-node",
  "version": "0.1.0",
  "description": "Basic node for request network v2.",
  "keywords": [
    "requestnetwork",
    "request-node"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/request-node",
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/server",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "nyc mocha --exit --require ts-node/register source-map-support/register test/**/*.ts",
    "start": "ts-node src/server.ts",
    "clean": "shx rm -rf dist",
    "lint-staged": "lint-staged",
    "lint": "tslint --project .",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "dependencies": {
    "@requestnetwork/data-access": "0.1.0",
    "@requestnetwork/ethereum-storage": "0.1.0",
    "@requestnetwork/types": "0.1.0",
    "express": "4.16.4",
    "http-status-codes": "1.3.0",
    "truffle-hdwallet-provider": "1.0.1",
    "yargs": "12.0.5"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/supertest": "2.0.7",
    "@types/yargs": "12.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "husky": "1.1.2",
    "lint-staged": "8.0.4",
    "mocha": "5.2.0",
    "prettier": "1.14.3",
    "shx": "0.3.2",
    "source-map-support": "0.5.9",
    "supertest": "3.3.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.3"
  }
}
