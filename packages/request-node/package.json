{
  "name": "@requestnetwork/request-node",
  "version": "0.1.0",
  "description": "Basic node for request network v2",
  "keywords": [
    "requestnetwork",
    "request-node"
  ],
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/request-node",
  "main": "dist/server",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "nyc mocha --require ts-node/register source-map-support/register test/**/*.ts",
    "start": "node ./dist/server.js",
    "clean": "shx rm -rf dist",
    "lint-staged": "lint-staged",
    "lint": "tslint --project .",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "dependencies": {
    "express": "4.16.4",
    "http-status-codes": "1.3.0"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/supertest": "2.0.7",
    "chai": "4.2.0",
    "husky": "1.1.2",
    "lint-staged": "8.0.4",
    "mocha": "5.2.0",
    "prettier": "1.14.3",
    "shx": "0.3.2",
    "source-map-support": "0.5.9",
    "supertest": "3.3.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.3"
  }
}
