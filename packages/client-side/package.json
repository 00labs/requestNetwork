{
  "name": "@requestnetwork/client-side",
  "version": "0.1.0",
  "description": "Client side library for Request Network",
  "keywords": [
    "requestnetwork",
    "client-side"
  ],
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/client-side#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "scripts": {
    "build": "tsc -b",
    "build:web": "browserify src/index.ts -p tsify  -p tinyify --debug | exorcist dist/requestnetwork-client-side.min.js.map > dist/requestnetwork-client-side.min.js",
    "clean": "shx rm -rf dist",
    "lint": "tslint --project .",
    "lint-staged": "lint-staged",
    "test": "nyc mocha --require ts-node/register --require source-map-support/register --require amd-loader \"test/**/*.ts\"",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "amd-loader": "0.0.8",
    "axios-mock-adapter": "1.15.0",
    "chai": "4.2.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "prettier": "1.14.3",
    "shx": "0.3.2",
    "source-map-support": "0.5.9",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.3"
  },
  "dependencies": {
    "@requestnetwork/data-access": "0.1.0",
    "@requestnetwork/request-logic": "0.1.0",
    "@requestnetwork/types": "0.1.0",
    "axios": "0.18.0",
    "browserify": "16.2.3",
    "exorcist": "1.0.1",
    "tinyify": "2.5.0",
    "tsify": "4.0.1"
  }
}
