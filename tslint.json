{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
      "indent": [true, "spaces", 4],
      "quotemark": [true, "single"],
      "curly": [true],
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "max-line-length": [false],
      "arrow-parens": [false],
      "no-var-requires": [false]
    }
}
