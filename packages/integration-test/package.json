{
  "name": "@requestnetwork/integration-test",
  "version": "0.30.0",
  "private": true,
  "description": "Integration tests for the request system.",
  "keywords": [
    "requestnetwork",
    "integration-test"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "test"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "lint": "eslint \"test/**/*.ts\"",
    "test": "run-s test:node test:layers",
    "test:scheduled": "run-s test:erc20 test:erc777 test:btc ",
    "test:layers": "jest test/layers.test.ts  --forceExit",
    "test:node": "jest test/node-client.test.ts --forceExit",
    "test:erc20": "jest test/scheduled/erc20*.test.ts --forceExit",
    "test:erc777": "jest test/scheduled/erc777*.test.ts --forceExit",
    "test:btc": "jest test/scheduled/btc.test.ts --forceExit"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "0.5.6",
    "@huma-shan/advanced-logic": "0.36.1",
    "@requestnetwork/currency": "0.10.0",
    "@huma-shan/data-access": "0.28.1",
    "@requestnetwork/epk-decryption": "0.5.0",
    "@requestnetwork/epk-signature": "0.7.0",
    "@huma-shan/ethereum-storage": "0.28.1",
    "@requestnetwork/multi-format": "0.17.0",
    "@huma-shan/payment-detection": "0.37.1",
    "@huma-shan/payment-processor": "0.39.1",
    "@huma-shan/request-client.js": "0.41.1",
    "@huma-shan/request-logic": "0.28.1",
    "@huma-shan/transaction-manager": "0.28.1",
    "@huma-shan/types": "0.37.1",
    "@requestnetwork/utils": "0.37.0",
    "@truffle/hdwallet-provider": "1.2.3",
    "@types/jest": "26.0.13",
    "@types/node": "14.14.16",
    "ethers": "5.5.1",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "web3-eth": "1.3.6"
  }
}
