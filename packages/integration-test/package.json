{
  "name": "@frinkly/integration-test",
  "version": "0.30.4",
  "private": true,
  "description": "Integration tests for the request system.",
  "keywords": [
    "requestnetwork",
    "integration-test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/integration-test#readme",
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "test"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "lint": "eslint \"test/**/*.ts\"",
    "test": "run-s test:node test:layers",
    "test:scheduled": "run-s test:erc20 test:erc777 test:btc ",
    "test:layers": "jest test/layers.test.ts  --forceExit",
    "test:node": "jest test/node-client.test.ts --forceExit",
    "test:erc20": "jest test/scheduled/erc20*.test.ts --forceExit",
    "test:erc777": "jest test/scheduled/erc777*.test.ts --forceExit",
    "test:btc": "jest test/scheduled/btc.test.ts --forceExit"
  },
  "devDependencies": {
    "@frinkly/advanced-logic": "0.36.1",
    "@frinkly/currency": "0.10.1",
    "@frinkly/data-access": "0.28.1",
    "@frinkly/epk-decryption": "0.4.1",
    "@frinkly/epk-signature": "0.6.1",
    "@frinkly/ethereum-storage": "0.28.1",
    "@frinkly/multi-format": "0.16.0",
    "@frinkly/payment-detection": "0.37.1",
    "@frinkly/payment-processor": "0.39.1",
    "@frinkly/request-client.js": "0.41.4",
    "@frinkly/request-logic": "0.27.1",
    "@frinkly/transaction-manager": "0.28.1",
    "@frinkly/types": "0.37.0",
    "@frinkly/utils": "0.37.1",
    "@openzeppelin/test-helpers": "0.5.6",
    "@truffle/hdwallet-provider": "1.2.3",
    "@types/jest": "26.0.13",
    "@types/node": "16.11.7",
    "ethers": "5.5.1",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "ts-jest": "26.3.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "web3-eth": "1.3.6"
  }
}
